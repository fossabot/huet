{
  "pallet": {
    "black": "#000000",
    "white": "#ffffff",
    "primary": "#a424b7",
    "link": "#1b52e0",
    "warning": "#cf6d18",
    "error": "#dd0000",
    "success": "#1d8835"
  },
  "ramps": {
    "neutral": {
      "colors": ["black", "white"],
      "classes": 5,
      // "gamma": 1,
      "correctLightness": true,
      "colorModel": "hcl", // lrgb, lab
      "bezier": false,
      "mode": "contrastNeutral" // direct, contrastColored, directOffset
    },
    "warning": {
      "colors": ["black", "warning", "white"],
      "type": "hue" // normal
    }
  },
  "bgColor": "white",
  "contrastMultiplier": 1,
  "saturationContrastMultiplier": 1,
  "allowedContrasts": [4, 8, 16, 32, 64, 128],
  "variations": {
    "disabled": {
      "contrastMultiplier": 0.5,
      "saturationContrastMultiplier": 0.5
    },
    "active": {
      "contrastMultiplier": 1.5
    },
    "invert": {
      "bgColor": "black"
    },
    "superContrast": {
      "isSuperContrast": true,
      "bgColor": "black"
    }
  }
}
