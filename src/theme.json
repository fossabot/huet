{
  "pallet": {
    "black": "#000000",
    "white": "#ffffff",
    "primary": "#a424b7",
    "link": "#1b52e0",
    "warning": "#cf6d18",
    "error": "#dd0000",
    "success": "#1d8835"
  },
  "ramps": {
    "base": {
      "colors": ["black", "white"],
      "classes": 5,
      // "gamma": 1,
      "correctLightness": true,
      "colorModel": "hcl", // lrgb, lab
      "bezier": false,
      "mode": "contrast.neutral" // direct, contrast.colored, directOffset
    },
    "backgroundGray": [
      {
        "ramp": "base",
        "scaleValue": 0.5
      },
      {
        "ramp": "base",
        "scaleValue": 1
      }
    ],
    "warning": {
      "colors": ["black", "warning", "white"]
    }
  },
  "bgColor": "white",
  "contrastMultiplier": 1,
  "saturationContrastMultiplier": 1,
  "allowedContrasts": [4, 8, 16, 32, 64, 128], // biggest number represents 100% contrast
  "variations": {
    "disabled": {
      "contrastMultiplier": 0.5,
      "saturationContrastMultiplier": 0.5
    },
    "active": {
      "contrastMultiplier": 1.5
    },
    "invert": {
      "bgColor": "black"
    },
    "superContrast": {
      "isSuperContrast": true,
      "bgColor": "black"
    }
  }
}
